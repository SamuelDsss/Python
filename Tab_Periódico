Obs:Só são 6 elementos da Tabela Periódico

print ("Tabela Periódico")

elemento = input("Digite aqui o elemento: ")
elemento = elemento.capitalize()

elemento1_nome = "agua"
elemento1_massa = 1
elemento1_numero = 18
elemento1 = 'H2o'

elemento2_nome = "cobre"
elemento2_massa = 63.546
elemento2_numero = 29
elemento2 = 'Cu'

elemento3_nome = "Cobalto"
elemento3_massa = 58.933
elemento3_numero = 27
elemento3 = 'Co'

elemento4_nome = "Chumbo"
elemento4_massa = 207.2
elemento4_numero = 82 
elemento4 = 'Pb'

elemento5_nome = "Carbono"
elemento5_massa = 12.011
elemento5_numero = 6
elemento5 = 'C'

elemento6_nome = "Hélio"
elemento6_massa = 4.003
elemento6_numero = 2
elemento6 = 'He'

if(elemento == elemento1):
    print(elemento1_nome)
    print(elemento1_massa)
    print(elemento1_numero)
elif(elemento == elemento2):
    print(elemento2_nome)
    print(elemento2_massa)
    print(elemento2_numero)
elif(elemento == elemento3):
    print(elemento3_nome)
    print(elemento3_massa)
    print(elemento3_numero)
elif(elemento == elemento4):
    print(elemento4_nome)
    print(elemento4_massa)
    print(elemento4_numero)
elif(elemento == elemento5):
    print(elemento5_nome)
    print(elemento5_massa)
    print(elemento5_numero)
elif(elemento == elemento6):
    print(elemento6_nome)
    print(elemento6_massa)
    print(elemento6_numero)
